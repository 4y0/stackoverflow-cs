{
  "name": "Stackoverflow Copier",
  "description": "Adds copy button / edit on codepen button to Stackoverflow codes",
  "version": "1.0",
  "permissions": [],
  "background": {
    "scripts": ["bg.js"]
  },
  "icons": { "16": "cs.png" },
  "content_scripts": [{
    "matches": ["*://stackoverflow.com/questions/*"],
    "js": ["cs.js"],
    "run_at": "document_end",
    "all_frames": true
    }],
  "manifest_version": 2
}